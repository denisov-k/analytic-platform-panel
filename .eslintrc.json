{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "commonjs": true
  }
}